{
  "version": 2,
  "builds": [
    {
      "src": "front/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "picsum-cache-api/src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/images/(.*)",
      "dest": "picsum-cache-api/src/main.ts"
    },
    {
      "src": "/api/images",
      "dest": "picsum-cache-api/src/main.ts"
    },
    {
      "src": "/(.*)",
      "dest": "front/$1"
    }
  ],
  "functions": {
    "picsum-cache-api/src/main.ts": {
      "maxDuration": 30
    }
  }
}
